# Application
NODE_ENV=development
APP_URL=http://localhost:3000
API_URL=http://localhost:8080
MCP_WS_URL=ws://localhost:8081

# Database
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/postgres
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here
# Get Supabase keys from: https://supabase.com/dashboard/project/_/settings/api

# Redis
REDIS_URL=redis://redis:6379

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_PUBLISHABLE_KEY=pk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx
STRIPE_PRICE_PREMIUM=price_xxx
STRIPE_PRICE_PRO=price_xxx
# Get Stripe keys from: https://dashboard.stripe.com/test/apikeys

# Email Service (Resend)
RESEND_API_KEY=re_xxx
EMAIL_FROM=noreply@helmwise.co
# Get Resend API key from: https://resend.com/api-keys

# Weather APIs
NOAA_API_KEY=your-noaa-key-here
# NOAA API is free, register at: https://www.ncdc.noaa.gov/cdo-web/token

OPENWEATHER_API_KEY=your-openweather-key-here
# Get OpenWeather API key from: https://home.openweathermap.org/api_keys

# Marine APIs
MARINETRAFFIC_API_KEY=your-marinetraffic-key-here
# Get MarineTraffic API key from: https://www.marinetraffic.com/en/ais-api-services

# Analytics
ANALYTICS_ENABLED=true
POSTHOG_KEY=your-posthog-key-here
# Get PostHog key from: https://posthog.com/

# Security
JWT_SECRET=your-jwt-secret-here-generate-a-random-string
ENCRYPTION_KEY=your-encryption-key-here-generate-a-random-string

# Frontend Environment Variables (exposed to client)
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_WS_URL=ws://localhost:8081
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

