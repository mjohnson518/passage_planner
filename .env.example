# Application
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Database
DATABASE_URL=postgresql://admin:secure_password@localhost:5432/passage_planner

# Redis
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Authentication (choose one)
# Option 1: Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Option 2: Auth0
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret
AUTH0_DOMAIN=your-domain.auth0.com

# Stripe Payment Processing
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PREMIUM_MONTHLY_PRICE_ID=price_...
STRIPE_PREMIUM_YEARLY_PRICE_ID=price_...
STRIPE_PRO_MONTHLY_PRICE_ID=price_...
STRIPE_PRO_YEARLY_PRICE_ID=price_...

# External APIs
NOAA_API_KEY=your_noaa_api_key
OPENWEATHER_API_KEY=your_openweather_api_key
WINDY_API_KEY=your_windy_api_key

# LLM Integration (optional)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Email Service
RESEND_API_KEY=re_...
EMAIL_FROM=hello@passageplanner.com

# Analytics
MIXPANEL_TOKEN=...
POSTHOG_API_KEY=...
SENTRY_DSN=...

# Feature Flags
LAUNCHDARKLY_SDK_KEY=...

# MCP Configuration
MCP_ORCHESTRATOR_PORT=8080
MCP_AGENT_TIMEOUT=30000

# Frontend URLs
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_WS_URL=ws://localhost:8080

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001

# Health Check
HEALTH_PORT=8081

# Production URLs (required for CSRF protection)
NEXT_PUBLIC_APP_URL=https://your-domain.com
VERCEL_URL=your-vercel-deployment.vercel.app
